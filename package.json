{
  "name": "h3-proxy",
  "version": "1.6.0",
  "description": "A proxy event handler for h3, using proxyRequest.",
  "type": "module",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "exports": {
    ".": {
      "import": "./lib/index.esm.js",
      "require": "./lib/index.cjs.js",
      "types": "./lib/index.d.ts"
    }
  },
  "scripts": {
    "dev": "rollup -c -w",
    "build": "rollup -c",
    "release": "npm run build && changelogen --release && npm publish --access=public && git push --follow-tags",
    "test:dev": "vitest dev",
    "test": "vitest run",
    "test:coverage": "yarn test -- --reporter verbose --coverage",
    "start": "node ./server/main.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yisibell/h3-proxy.git"
  },
  "keywords": [
    "h3",
    "http-proxy",
    "event-handler",
    "http",
    "proxy",
    "proxyRequest",
    "middleware"
  ],
  "author": {
    "name": "elenh",
    "email": "yisiwings@163.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/yisibell/h3-proxy/issues"
  },
  "homepage": "https://github.com/yisibell/h3-proxy#readme",
  "dependencies": {
    "consola": "^2.15.3",
    "is-glob": "^4.0.3",
    "micromatch": "^4.0.5"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^4.0.3",
    "@rollup/plugin-commonjs": "^24.0.0",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@rollup/plugin-typescript": "^10.0.1",
    "@types/is-glob": "^4.0.2",
    "@types/lodash": "^4.14.191",
    "@types/micromatch": "^4.0.2",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.46.1",
    "@typescript-eslint/parser": "^5.46.1",
    "changelogen": "^0.4.1",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "lodash": "^4.17.21",
    "prettier": "^2.8.1",
    "rollup": "^3.7.5",
    "rollup-plugin-dts": "^5.1.1",
    "supertest": "^6.3.3",
    "tslib": "^2.4.1",
    "typescript": "~4.9.4",
    "vitest": "^0.29.2",
    "h3": "^1.6.0"
  },
  "peerDependencies": {
    "h3": "^1.6.0"
  },
  "engines": {
    "node": ">=18.x"
  }
}
